#include <stdio.h>

int main()
{
	
		int a, b, c;

	printf("세수를 입력하세요.\n");
	scanf_s("%d%d%d", &a, &b, &c);

	if (a > b&& a > c)
	{
		printf("%d가 가장 큽니다.\n", a);
	}
	else if (b > a&& b > c)
	{
		printf("%d가 가장 큽니다.\n", b);
	}
	else if (c > a&& c > b)
	{
		printf("%d가 가장 큽니다.\n", c);
	}
	else if (a == b && b > c)
	{
		printf("%d와%d가 같고 큽니다.\n", a, b);
	}
	else if (b == c && b > a)
	{
		printf("%d와%d가 같고 큽니다.\n", b, c);
	}
	else
	{
		printf("%d와%d와%d가 다 같니다.\n", a, b, c);
	}

	return 0;
}

